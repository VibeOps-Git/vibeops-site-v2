{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "app.py": {
      "excludeFiles": "{public/**,__pycache__/**,*.cache/**}"
    }
  },
  "routes": [
    {
      "src": "/videos/(.*)",
      "dest": "/public/videos/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}